{
  "name": "product-catalog-admin-vue",
  "version": "0.1.0",
  "description": "Vue.js SPA admin client for Product Catalog website project",
  "author": "David Tsai <chiayu.tsai.personal@gmail.com>",
  "private": false,
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["vue-cli-service lint", "git add"],
    "*.vue": ["vue-cli-service lint", "git add"]
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "@vue/eslint-config-prettier": "^3.0.0-beta.10",
    "lint-staged": "^7.1.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build":
      "vue-cli-service build --dest \"../product-catalog-admin-express/public\"",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  }
}
